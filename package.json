{
  "name": "mavensmate",
  "displayName": "mavensmate",
  "description": "MavensMate plugin for the Visual Studio Code editor",
  "version": "0.0.1",
  "publisher": "DavidHelmer",
  "engines": {
    "vscode": "^0.10.10"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      },
      {
        "command": "mavensmate.openProject",
        "title": "Open Project"
      },
      {
        "command": "mavensmate.openUI",
        "title": "Open MavensMate UI"
      },
      {
        "command": "mavensmate.newProject",
        "title": "New Project"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "bluebird": "3.3.5",
    "fs-promise": "0.5.0",
    "path": "0.12.7",
    "request-promise": "2.0.1",
    "url-join": "1.1.0"
  },
  "devDependencies": {
    "expect.js": "0.3.1",
    "nock": "8.0.0",
    "sinon": "1.17.3",
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  }
}
